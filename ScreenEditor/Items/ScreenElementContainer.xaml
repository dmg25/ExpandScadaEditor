<local:ScreenElement x:Class="ExpandScadaEditor.ScreenEditor.Items.ScreenElementContainer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ExpandScadaEditor.ScreenEditor.Items"
             xmlns:helpers="clr-namespace:ExpandScadaEditor.ScreenEditor.WorkspaceHelperControls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid x:Name="RootContainer">
        <helpers:ElementResizingBorder x:Name="RESIZE_BORDER"/>
        <helpers:ElementMovingBorder x:Name="MOVING_BORDER"/>
        <helpers:MouseOverElementBorder x:Name="COVER_BORDER"/>
    </Grid>
</local:ScreenElement>
